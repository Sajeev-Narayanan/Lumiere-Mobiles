        </div>
    </section>
   
<div class="container-fluid bootstrap snippets bootdey">
<div class="row">
  <div class="profile-nav col-md-3">
      <div class="panel">
          <div class="user-heading round">
              <a href="#">
                  <img src="https://cdn-icons-png.flaticon.com/512/1177/1177568.png" alt="">
              </a>
              <h1 class="text-light"><%= user.username %> </h1>
              <p><%= user.email %> </p>
          </div>

          <ul class="nav nav-pills flex-column">
              <li ><a href="/profile"> <i class="fa fa-user"></i> Profile</a></li>
              <li class="active"><a href="/changePassword"> <i class="fa fa-edit"></i> Change Password</a></li>
              <li><a href="/orders"> <i class="fa fa-calendar"></i> Orders </a></li>
          </ul>
      </div>
  </div>
  <div class="col-lg-6">
    <div class="login_form_inner">
     
      <form
        class="row login_form"
        action="/changePasswordPost"
        method="post"
        id="contactForm"
        
      >
        
        <div class="col-md-12 form-group">
          <input
            type="password"
            class="form-control"
            style="background-color: white"
            id="name"
            name="currentPassword"
            placeholder="Current Password"
            onfocus="this.placeholder = ''"
            onblur="this.placeholder = 'Current Password'"
            required
          />
        </div>
        <div class="col-md-12 form-group">
            <input
              type="password"
              class="form-control"
              style="background-color: white"
              id="name"
              name="newPassword"
              placeholder="New Password"
              onfocus="this.placeholder = ''"
              onblur="this.placeholder = 'New Password'"
              required
            />
          </div>
          <div class="col-md-12 form-group">
            <input
              type="password"
              class="form-control"
              style="background-color: white"
              id="name"
              name="confirmPassword"
              placeholder="Confirm Password"
              onfocus="this.placeholder = ''"
              onblur="this.placeholder = 'Confirm Password'"
              required
            />
          </div>
          <div class="text-danger"><%= msag %></div>
          <div class="text-success"><%= msg %></div>
        

        <div class="col-md-12 form-group">
          <button type="submit" value="submit" class="primary-btn">
            Save Changes
          </button>
        </div>
      </form>
    </div>
  </div>
  </div>
  </div>
